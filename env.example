# Strapi API Configuration
# Copy this file to .env and update the values for your local development

# Strapi API URL (without /api suffix)
STRAPI_API_URL=https://your-strapi-instance.com/api

# Strapi API Token for authentication
STRAPI_API_TOKEN=your-api-token-here

# Strapi Media URL (base URL for media files)
STRAPI_MEDIA_URL=https://your-strapi-instance.com